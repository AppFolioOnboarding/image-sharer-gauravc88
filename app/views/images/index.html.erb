<h2>Stored Images</h2>
<div class="card-columns">
  <% @images.each do |image| %>
    <div class="card">
      <%= link_to (image_tag image.url, class: 'card-img'), image %>
      <div class="card-text">
        <% image.tag_list.each do |tag| %>
          <a href=<%= images_path + '?tag=' + tag %>>#<%= tag %></a>
        <% end %>
      </div>
    </div>
  <% end %>
</div>
<%= link_to 'New Image', new_image_path %>
